<!doctype html>
<html>
<head>
    <meta charset="UTF-8">
    <title>后台管理</title>
    <link rel="stylesheet" type="text/css" href="/stylesheets/common.css"/>
    <link rel="stylesheet" type="text/css" href="/stylesheets/main.css"/>
    <script type="text/javascript" src="/javascripts/libs/modernizr.min.js"></script>
</head>
<body>
<div class="topbar-wrap white">
    <div class="topbar-inner clearfix">
        <div class="topbar-logo-wrap clearfix">
            <h1 class="topbar-logo none"><a href="/" class="navbar-brand">自由行后台管理</a></h1>
            <ul class="navbar-list clearfix">
                <li><a class="on" href="/index">首页</a></li>
            </ul>
        </div>
        <div class="top-info-wrap">
            <ul class="top-info-list clearfix">
                <li><a href="/">退出</a></li>
            </ul>
        </div>
    </div>
</div>
<div class="container clearfix">
    <div class="sidebar-wrap">
        <div class="sidebar-title">
            <h1>菜单</h1>
        </div>
        <div class="sidebar-content">
            <ul class="sidebar-list">
                <li>
                    <a href="#"><i class="icon-font">&#xe003;</i>常用操作</a>
                    <ul class="sub-menu">
                        <li><a href="/users/QueryAllTraveal"><i class="icon-font">&#xe008;</i>游记管理</a></li>
                        <li><a href="/users/QueryAllStratagy"><i class="icon-font">&#xe005;</i>攻略管理</a></li>
                        <li><a href="/users/QueryAllintegral"><i class="icon-font">&#xe006;</i>积分管理</a></li>
                        <li><a href="/users/QueryAllUser"><i class="icon-font">&#xe004;</i>用户管理</a></li>
                        <li><a href="/users/QueryAllQuestion"><i class="icon-font">&#xe012;</i>问答管理</a></li>
                        <li><a href="/users/QueryAllPinglun"><i class="icon-font">&#xe052;</i>评论管理</a></li>
                        <li><a href="/users/QueryAllgoods"><i class="icon-font">&#xe052;</i>商品管理</a></li>
                        <li><a href="/users/Destination"><i class="icon-font">&#xe052;</i>目的地管理</a></li>
                    </ul>
                    </ul>
                </li>
            </ul>
        </div>
    </div>
    <!--/sidebar-->
    <div class="main-wrap">

        <div class="crumb-wrap">
            <div class="crumb-list">
                <i class="icon-font"></i>
                <a href="/">首页</a>
                <span class="crumb-step">&gt;</span>
                <span class="crumb-name">用户管理</span>
            </div>
        </div>
        <div class="result-wrap">
            <form name="myform" id="myform" method="post" >
                <div class="result-content">
                    <!--实验-->
                    <% if(users.length>0){%>
                    <table class="result-tab" width="100%" style="text-align: center;" >
                        <tr style="text-align: center;">
                            <th>用户ID</th>
                            <th>用户名</th>
                            <th>性别</th>
                            <th>TEL</th>
                            <th>头像</th>
                        </tr>
                        <% for(var i=0;i<users.length;i++) {%>
                        <tr>
                            <td><%=users[i].UserId%></td>
                            <td><%=users[i].UserName%></td>
                            <td><%=users[i].Gender%></td>
                            <td><%=users[i].TEL%></td>
                            <td><img src="/upload/<%=users[i].UserPic%>" alt="" style="width: 150px;height:100px"></td>
                        </tr>
                        <% }%>
                    </table>
                    <% }%>
                </div>
            </form>
        </div>
    </div>
    <!--/main-->
</div>

</body>
</html>