<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
    <link rel="stylesheet" href="/stylesheets/Myhome_question.css"/>
    <script src="/javascripts/jquery-3.1.0.min.js"></script>
    <script src="/javascripts/SelectUserPic.js"></script>
    <script>
        window.onload=SelectUserPic()
    </script>
</head>
<body>
<header>
    <%- include ../util/header-All_Other.ejs %>
</header>
<section style="margin: auto;width: 960px">
    <div class="UserPic_divH">
        <div class="Pic_divHL">
            <img src="/upload/<%= myhomeQuestionData[0].UserPic %>" alt="">
        </div>
        <div class="UserName_divH"><span><%= myhomeQuestionData[0].UserName %></span></div>

    </div>
    <div class="navbarH">
        <ul>
            <a href="/users/Otherhome_note_here?username=<%= myhomeQuestionData[0].UserName %>">
                <li class="  navbarH_li">Ta的游记</li>
            </a>
            <a href="/users/Otherhome_question?username=<%= myhomeQuestionData[0].UserName %>">
                <li class="active">Ta的问答</li>
            </a>
            <a href="/users/Otherhome_exchange?username=<%= myhomeQuestionData[0].UserName %>">
                <li class="navbarH_li">Ta的兑换</li>
            </a>
            <a href="/users/OtherPersonal_setting?username=<%= myhomeQuestionData[0].UserName %>">
                <li class="navbarH_li">Ta的资料设置</li>
            </a>
        </ul>
    </div>
    <div class="travelNote_conH">
        <!-- <div class="travelAnswerH">
             <p>我的问答</p>
         </div>-->
        <div class="travel_askH">
            <% if(myhomeQuestionData.length > 0){ %>
            <% for(var i = 0;i < myhomeQuestionData.length;i++) { %>
            <div class="wenda_divH">
                <a href=""><p class="wenda_p"> <%= myhomeQuestionData[i].QuestionName %></p></a>
                <p class="wenda_QcH"><%= myhomeQuestionData[i].QuestionContent %></p>
                <p class="wenda_time">发表于 <span class="wenda_time2"><%= myhomeQuestionData[i].QuestionTime %></span></p>
                <!--<span class="wenda_delete"><a href="">删除</a></span>-->
            </div>
            <% } %>
            <% } %>
        </div>
    </div>
</section>
</body>
</html>